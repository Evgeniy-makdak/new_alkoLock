{
  "printWidth": 100,
  "arrowParens": "always",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^(react|react-.*)$", "^[a-zA-Z]", "^@mui", "^@", "^[./]", "^[^./].*\\.scss$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
